    <script src="~/About_us_ssets/js/jquery.min.js"></script>
    <script src="~/About_us_ssets/js/bootstrap.bundle.min.js"></script>
    <script src="~/About_us_ssets/js/jquery-3.0.0.min.js"></script>
    <!-- sidebar -->
    <script src="~/About_us_ssets/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/About_us_ssets/js/custom.js"></script>


</body>
</html>