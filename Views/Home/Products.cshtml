@model Tuple<IEnumerable<FURNITURE.Models.HomePageZ>,IEnumerable<FURNITURE.Models.CategoryZ>,IEnumerable<FURNITURE.Models.ProductZ>>

@{
    ViewData["Title"] = "Products";
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>





    <!--====== Required meta tags ======-->

    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width" />
    <title>Furnish</title>






     <!-- bootstrap css -->
      <link rel="stylesheet" href="~/About_us_assets/css/bootstrap.min.css">
      <!-- style css -->
      <link rel="stylesheet" href="~/About_us_assets/css/style.css">
      <!-- Responsive-->
      <link rel="stylesheet" href="~/About_us_assets/css/responsive.css">
      <!-- fevicon -->
      <link rel="icon" href="~/About_us_assets/images/fevicon.png" type="image/gif" />
      <!-- Scrollbar Custom CSS -->
      <link rel="stylesheet" href="~/About_us_assets/css/jquery.mCustomScrollbar.min.css">
      <!-- Tweaks for older IEs-->
      <link rel="stylesheet" href="~/About_us_assets/https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
      <link rel="stylesheet" href="~/About_us_assets/https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css" media="screen">
      <!--[if lt IE 9]>
      <script src="~/About_us_ssets/https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="~/About_us_ssets/https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->












    <!--====== Favicon Icon ======-->
    <link rel="shortcut icon" href="~/SharedAssets/assets/images/favicon.png" type="image/png">

    <!--====== Bootstrap css ======-->
    <link rel="stylesheet" href="~/SharedAssets/assets/css/bootstrap.min.css">

    <!--====== Animate css ======-->
    <link rel="stylesheet" href="~/SharedAssets/assets/css/animate.css">

    <!--====== Magnific Popup css ======-->
    <link rel="stylesheet" href="~/SharedAssets/assets/css/magnific-popup.css">

    <!--====== Slick css ======-->
    <link rel="stylesheet" href="~/SharedAssets/assets/css/slick.css">

    <!--====== Line Icons css ======-->
    <link rel="stylesheet" href="~/SharedAssets/assets/css/LineIcons.css">

    <!--====== Default css ======-->
    <link rel="stylesheet" href="~/SharedAssets/assets/css/default.css">

    <!--====== Style css ======-->
    <link rel="stylesheet" href="~/SharedAssets/assets/css/style.css">

    <!--====== Responsive css ======-->

    <link rel="stylesheet" href="~/SharedAssets/assets/css/responsive.css">




   
</head>
<body>
    <!--====== PRELOADER PART START ======-->

    <div class="preloader">
        <div class="spin">
            <div class="cube1"></div>
            <div class="cube2"></div>
        </div>
    </div>

    <!--====== PRELOADER PART START ======-->
    <!--====== HEADER PART START ======-->

    <header class="header-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <nav class="navbar navbar-expand-lg">
                        <a class="navbar-brand" asp-controller="Home" asp-action="Index">
                            @foreach(var item in Model.Item1)
                            {
                            <img src=@Url.Content("~/Images/" + item.Logo) asp-append-version="true" />
                            }
                        </a> <!-- Logo -->
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="bar-icon"></span>
                            <span class="bar-icon"></span>
                            <span class="bar-icon"></span>
                        </button>

                        <div class="collapse navbar-collapse" id="navbarSupportedContent" >
                            <ul id="nav" class="navbar-nav ml-auto">
                                 <li class="nav-item">
                                    <a data-scroll-nav="0" asp-controller="Home" asp-action="Index">Home</a>
                                </li>
                                <li class="nav-item">
                                    <a data-scroll-nav="0" asp-controller="Home" asp-action="Products">Products</a>
                                </li>
                                <li class="nav-item">
                                    <a data-scroll-nav="0" asp-controller="Home" asp-action="Gallery">Gallery</a>
                                </li>
                                <li class="nav-item">
                                    <a data-scroll-nav="0" asp-controller="Home" asp-action="Contact_us">Contact us</a>
                                </li>
                                <li class="nav-item">
                                    <a data-scroll-nav="0" asp-controller="Home" asp-action="About_us">About us</a>
                                </li>
                                 <li class="nav-item">
                                    <b style="color:coral;">|</b>
                                </li>
                                <li class="nav-item">
                                    @foreach(var item in Model.Item1)
                                    {
                                    <h1 style=" width: 100% ;  float: left;font-size: 16px; color:coral;padding-top:1px">
                                     Call Us : @item.Phone
                                    </h1>
                                    }
                                </li>
                                <li class="nav-item">
                                    <b style="color:coral;">|</b>
                                </li>
                                <!-- USER CHECK STARTS-->
                                @{
                                    if (ViewBag.loggedin == 60)
                                    {

                                        <li class="nav-item">
                                            <a data-scroll-nav="0" asp-controller="UserPages" asp-action="Discover">User Dashboard</a>
                                        </li>
                                    }

                                    else if (ViewBag.Adminloggedin == 70)
                                    {
                                        <li class="nav-item">
                                            <a data-scroll-nav="0" asp-controller="Statistics" asp-action="Statistics">Admin Dashboard</a>
                                        </li>
                                    }
                                    else
                                    {
                                        <li class="nav-item">
                                            <a data-scroll-nav="0" asp-controller="Authentication" asp-action="Login">Log in</a>
                                        </li>
                                        <li class="nav-item">
                                            <a data-scroll-nav="0" asp-controller="Authentication" asp-action="Register">Register</a>
                                        </li>

                                    }
                                }
                                <!--USER CHECK ENDS-->
                                    

                               
                            </ul> <!-- navbar nav -->
                        </div>
                    </nav> <!-- navbar -->
                </div>
            </div> <!-- row -->
        </div> <!-- container -->
    </header>

    <!--====== HEADER PART ENDS ======-->
    
    <!--====== SHOWCASE PART START ======-->
    
    <section id="showcase" class="showcase-area pt-100 pb-130">
        <div class="container">
            <div class="row">
                <div class="col-lg-4">
                    <div class="showcase-title pt-25">
                        @foreach (var item in Model.Item1)
                        {
                            <h2 class="title" style="color:coral">@item.Text1</h2>
                        }
                    </div> <!-- showcase title -->
                </div> 
                <div class="col-lg-6">
                    <div class="showcase-title pt-25">
                        @foreach (var item in Model.Item1)
                        {
                            <p>@item.Paragraph</p>
                        }
                    </div> <!-- showcase title -->
                </div>
            </div> <!-- row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="showcase-active mt-65">
                        <div class="single-showcase">
@*  
                           <img src="~/SharedAssets/assets/images/showcase/t-1.jpg">
                            <img src="~/SharedAssets/assets/images/showcase/t-2.jpg">
*@
                             @foreach (var item in Model.Item1)
                             {
                                <img src=@Url.Content("~/Images/" + item.Image) asp-append-version="true" />
                             }
                            <a href="#" class="main-btn" style="font-size:20px;">Full Preview</a>

                        </div> <!-- single showcase -->
                        <div class="single-showcase">
                            @foreach (var item in Model.Item1)
                            {
                                <img src=@Url.Content("~/Images/" + item.Image2) asp-append-version="true" />
                            }
                            <a href="#" class="main-btn" style="font-size:20px;">Full Preview</a>

                        </div> <!-- single showcase -->
                    </div> <!-- showcase active -->
                </div>
            </div> <!-- row -->
        </div> <!-- container -->
    </section>
    
    <!--====== SHOWCASE PART ENDS ======-->
   

<!--====== DISCOUNT PRODUCT PART START ======-->

<section id="discount-product" class="discount-product pt-100">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="single-discount-product mt-30">
                    <div class="product-image">
                        <img src="~/SharedAssets/assets/images/discount-product/product-1.jpg" alt="Product">
                    </div> <!-- product image -->
                    <div class="product-content">
                        <h4 class="content-title mb-15">High-quality furnitures <br> For your home</h4>
                    </div> <!-- product content -->
                </div> <!-- single discount product -->
            </div>
            <div class="col-lg-6">
                <div class="single-discount-product mt-30">
                    <div class="product-image">
                        <img src="~/SharedAssets/assets/images/discount-product/product-2.jpg" alt="Product">
                    </div> <!-- product image -->
                    <div class="product-content">
                        <h4 class="content-title mb-15">Hot Offer <br> Discount up to 80%</h4>
                    </div> <!-- product content -->
                </div> <!-- single discount product -->
            </div>
        </div> <!-- row -->
    </div> <!-- container -->
</section>

<!--====== DISCOUNT PRODUCT PART ENDS ======-->

    <!--====== PRODUCT PART START ======-->

    <section id="product" class="product-area pt-100 pb-130">
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-4">
                    <div class="collection-menu text-center mt-30">
                        <h4 class="collection-tilte">OUR COLLECTION</h4>
                        <div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">

                            @foreach(var item in Model.Item2)
                            {
                                 
                            <a class="active" id="v-pills-furniture-tab" data-toggle="pill" href="#v-pills-furniture" role="tab" aria-controls="v-pills-furniture" aria-selected="true">@item.Name</a>
                             <hr style="font-weight:bolder" />
                            }



                        </div> <!-- nav -->
                    </div> <!-- collection menu -->
                </div>
                <div class="col-lg-9 col-md-8" style="margin-top: 95px;">
                    <div class="tab-content" id="v-pills-tabContent">
                        <div class="tab-pane fade show active" id="v-pills-furniture" role="tabpanel" aria-labelledby="v-pills-furniture-tab">
                            <div class="product-items mt-30">
                                <div class="row product-items-active">

                                    @foreach (var item in Model.Item3)
                                    {
                                        <div class="col-md-4">
                                            <div class="single-product-items">
                                                <div class="product-item-image">
                                                        <img class="card-img-top" src=@Url.Content("~/Images/" + item.Image) asp-append-version="true">
                                                </div>
                                                <div class="product-item-content text-center mt-30">
                                                    <h5 class="product-title"><a href="#">@item.Name</a></h5>
                                                    <ul class="rating">
                                                        <li><i class="lni-star-filled"></i></li>
                                                        <li><i class="lni-star-filled"></i></li>
                                                        <li><i class="lni-star-filled"></i></li>
                                                        <li><i class="lni-star-filled"></i></li>
                                                    </ul>
                                                    <span class="regular-price">$@item.Price</span>
                                                </div>
                                            </div> <!-- single product items -->
                                        </div>
                                    }
                                   
                                </div> <!-- row -->
                            </div> <!-- product items -->
                        </div> <!-- tab pane -->

                       
                    </div> <!-- tab content --> 
                </div>
            </div> <!-- row -->
        </div> <!-- container -->
    </section>

    <!--====== PRODUCT PART ENDS ======-->
    <!--====== FOOTER PART START ======-->

    <section id="footer" class="footer-area">
        <div class="container">
            <div class="footer-widget pt-75 pb-120">
                <div class="row">
                    <div class="col-lg-3 col-md-5 col-sm-7">
                        <div class="footer-logo mt-40">
                            <a href="#">
                                @foreach (var item in Model.Item1)
                                {
                                    <img src=@Url.Content("~/Images/" + item.Logo) asp-append-version="true" />
                                }
                            </a>
                            <p class="mt-10">Follow us</p>
                            <ul class="footer-social mt-25">
                                <li><a href="#"><i class="lni-facebook-filled"></i></a></li>
                                <li><a href="#"><i class="lni-twitter-original"></i></a></li>
                                <li><a href="#"><i class="lni-instagram"></i></a></li>
                            </ul>
                        </div> <!-- footer logo -->
                    </div>
                    <div class="col-lg-3 col-md-4 col-sm-5">
                        <div class="footer-link mt-50">
                            <h5 class="f-title" style="font-size:20px;">Services</h5>
                            <ul>
                                <li><a href="#">Retina Ready</a></li>    
                                <li><a href="#">Creative Elements</a></li>
                                <li><a href="#"> Easy-to-Use</a></li>
                                <li><a href="#">   Easy Import</a></li>
                                <li><a href="#">  Bedroom</a></li>
                                <li><a href="#">   Office</a></li>
                            </ul>
                        </div> <!-- footer link -->
                    </div>
                    <div class="col-lg-2 col-md-3 col-sm-5">
                        <div class="footer-link mt-50">
                            <h5 class="f-title" style="font-size:20px;">Help</h5>
                            <ul>
                                <li><a href="#">Help Center</a></li>
                                <li><a asp-controller="Home" asp-action="Contact_us">Contact Us</a></li>
                                <li><a href="#">Privacy Policy</a></li>
                            </ul>
                        </div> <!-- footer link -->
                    </div>
                    <div class="col-lg-4 col-md-5 col-sm-7">
                        <div class="footer-info mt-50">
                            <h5 class="f-title" style="font-size:20px;">Contact Info</h5>
                            <ul>
                                <li>
                                    <div class="single-footer-info mt-20">
                                        <span>Phone :</span>
                                        <div class="footer-info-content">
                                            @foreach (var item in Model.Item1)
                                            {
                                                <p>@item.Phone</p>
                                            }
                                        </div>
                                    </div> <!-- single footer info -->
                                </li>
                                <li>
                                    <div class="single-footer-info mt-20">
                                        <span>Email :</span>
                                        <div class="footer-info-content">
                                            @foreach (var item in Model.Item1)
                                            {
                                                <p>@item.Email</p>
                                            }
                                        </div>
                                    </div> <!-- single footer info -->
                                </li>
                                <li>
                                    <div class="single-footer-info mt-20">
                                        <span>Address :</span>
                                        <div class="footer-info-content">
                                            @foreach (var item in Model.Item1)
                                            {
                                                <p>@item.Address</p>
                                            }
                                        </div>
                                    </div> <!-- single footer info -->
                                </li>
                            </ul>
                        </div> <!-- footer link -->
                    </div>
                </div> <!-- row -->
            </div> <!-- footer widget -->
            <div class="footer-copyright pt-15 pb-15">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="copyright text-center">
                            <p>
                                &copy; <span style="color:coral;">Copyright</span> 2023 all rights reserved</>
                            </p>
                        </div> <!-- copyright -->
                    </div>
                </div> <!-- row -->
            </div> <!--  footer copyright -->
        </div> <!-- container -->
    </section>

    <!--====== FOOTER PART ENDS ======-->
    <!--====== BACK TO TOP PART START ======-->

    <a href="#" class="back-to-top"><i class="lni-chevron-up"></i></a>

    <!--====== BACK TO TOP PART ENDS ======-->
