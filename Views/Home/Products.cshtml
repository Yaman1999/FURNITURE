    <script src="~/SharedAssets/assets/js/vendor/modernizr-3.6.0.min.js"></script>
    <script src="~/SharedAssets/assets/js/vendor/jquery-1.12.4.min.js"></script>

    <!--====== Bootstrap js ======-->
    <script src="~/SharedAssets/assets/js/bootstrap.min.js"></script>

    <!--====== Slick js ======-->
    <script src="~/SharedAssets/assets/js/slick.min.js"></script>

    <!--====== Magnific Popup js ======-->
    <script src="~/SharedAssets/assets/js/jquery.magnific-popup.min.js"></script>

    <!--====== nav js ======-->
    <script src="~/SharedAssets/assets/js/jquery.nav.js"></script>

    <!--====== Scroll It js ======-->
    <script src="~/SharedAssets/assets/js/scrollIt.min.js"></script>

    <!--====== Main js ======-->


    <script src="~/SharedAssets/assets/js/main.js"></script>







    @*    ------------------------------------
